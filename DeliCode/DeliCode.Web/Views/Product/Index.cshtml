@model List<DeliCode.Web.Models.Product>
@{
    ViewData["Title"] = "Produkter";
}

<div class="container">
    <div class="row justify-content-center">
        @foreach (var item in Model)
        {
            <div class="col-12 col-sm-8 col-md-6 col-lg-4 mt-2 mb-2">
                @await Html.PartialAsync("_ProductPartial", item)
            </div>
        }
    </div>
</div>
