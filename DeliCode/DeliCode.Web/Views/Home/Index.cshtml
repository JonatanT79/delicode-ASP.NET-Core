@model List<Product>
@{
    ViewData["Title"] = "Välkommen till Deli Code!";
}

<div class="jumbotron jumbotron-fluid jumbotron-custom">
    <div class="container">
        <div class="jumboheader text-center">
            <h1 class="display-4">Beställ en av våra godsaker!</h1>
            <p class="lead">Vi erbjuder hemleverans och catering</p>
        </div>
    </div>
</div>
<div class="text-center mb-5">
    <h3 class="display-4">Godaste grejjerna i stan</h3>

    <hr class="align-middle" />
</div>
